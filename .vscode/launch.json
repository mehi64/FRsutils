            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: plot_smote",
            "type": "debugpy",
            "request": "launch",
            "module": "FRsutils.utils.plot_SMOTE",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: experiment_1_non_pipeline",
            "type": "debugpy",
            "request": "launch",
            "module": "experiments.experiment_1_non_pipeline.py",
